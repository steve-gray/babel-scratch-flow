{
  "name": "babel-scratch",
  "version": "0.1.0",
  "description": "Flowtype scratch project with babel",
  "main": "dist/index.js",
  "scripts": {
    "start": "babel-node src",
    "test": "gulp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steve-gray/babel-scratch-flow/"
  },
  "keywords": [
  ],
  "author": "linehaul.cd team",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/steve-gray/babel-scratch-flow/issues"
  },
  "homepage": "https://github.com/steve-gray/babel-scratch-flow#readme",
  "dependencies": {
    "babel-core": "^6.9.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-syntax-export-extensions": "^6.8.0",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-1": "^6.5.0",
    "connect": "^3.4.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "eslint": "^2.13.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "flow-bin": "^0.27.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^2.0.0",
    "gulp-flow": "^1.2.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1"
    ],
    "plugins": [
      "add-module-exports",
      "transform-es2015-modules-commonjs",
      "transform-flow-strip-types",
      "syntax-export-extensions",
      "syntax-flow"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "ecmaFeatures": {
      "modules": true
    },
    "env": {
      "es6": true,
      "mocha": true,
      "node": true
    },
    "extends": "airbnb"
  }
}
